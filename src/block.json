{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mikelaroy/heroicons",
	"version": "0.1.0",
	"title": "Heroicons Block",
	"category": "common",
	"description": "A Gutenberg block for SVGs from Hericons",
	"supports": {
		"html": false,
		"align": true,
		"alignWide": false,
		"color": true,
		"anchor": true,
		"backgroundColor": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "mike-heroicons",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"attributes": {
		"svg": {
		  "type" : "string"
		},
		"style": {
			"type" : "string"
		}
	},
	"example": {
		"attributes":{
		  "svg": "clipboard-document",
		  "style": "solid"
		}
	}
}
